{
    "manifest_version": 2,

    "name": "tab_tmp",
    "version": "0.1",
    "description": "タブ操作をちょっと便利にしたいエクステンション！",

    "permissions": [
        "tabs", "http://*/*", "bookmarks"
    ],

    "browser_action": {
        "default_icon": "images/icon19.png",
        "default_title": "tab_tmp",
        "default_popup": "popup.html"
    }
}
