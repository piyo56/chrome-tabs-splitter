{
    "manifest_version": 2,

    "name": "tabs_splitter",
    "version": "0.2",
    "description": "タブ操作をちょっと便利にしたいエクステンション！",

    "permissions": [
        "tabs", "http://*/*", "bookmarks"
    ],

    "browser_action": {
        "default_icon": "images/icon1.png",
        "default_title": "tabs_splitter",
        "default_popup": "popup.html"
    }
}
